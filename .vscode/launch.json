{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Web",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d", "chrome",
                "--web-hostname", "localhost",
                "--web-port", "8080"
            ]
        },
        {
            "name": "Debug Develop",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "toolArgs": [
                "--flavor",
                "develop",
                "--dart-define-from-file=env/develop.json"
            ]
        },
        {
            "name": "Debug Qa",
